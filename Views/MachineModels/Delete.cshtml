@model CarManufactoring.Models.MachineModel

@{
    ViewData["Title"] = "Delete Machine Model";
}

<h2>@ViewBag.title</h2>

<h3>Are you sure you want to delete this?</h3>

@{
    await Html.RenderPartialAsync("_MachineModelsDetails", Model);
}
    
<form asp-action="Delete">
    <input type="hidden" asp-for="MachineModelId" />
    <input type="submit" value="Delete" class="btn btn-danger" />
    <a asp-action="Index" class="btn btn-secondary">Cancel</a>
</form>


