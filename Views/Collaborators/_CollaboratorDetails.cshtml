@model Collaborator

<dl class="row">
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model => model.Name)
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.Name)
    </dd>
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model => model.BirthDate)
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.BirthDate)
    </dd>
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model => model.Phone)
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.Phone)
    </dd>
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model => model.Email)
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.Email)
    </dd>
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model => model.Genders)
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.Genders.GenderDefinition)
    </dd>
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model => model.OnDuty)
    </dt>
    @if (Model.OnDuty)
    {
        <dd class="col-sm-10">On Duty</dd>
    }
    else
    {
        <dd class ="col-sm-10">Off Duty</dd>
    }
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model => model.Status)
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.Status)
    </dd>
</dl>